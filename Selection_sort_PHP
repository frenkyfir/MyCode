<?php
echo "selection sort";
$data=array(3,4,2,5,1);
echo "<br>";
echo "sebelum di sorting ";
print_r($data);
function selection_sort($data){
  $n=count($data);
  for ($i = 0;$i<$n;$i++){
    $k = $i;
    for ($j = $i+1;$j<$n;$j++){
      if ($data[$j] < $data[$k]) $k = $j;
    }   
    $dummy=$data[$i];
    $data[$i]=$data[$k];
    $data[$k]=$dummy;
  }
  return $data;
}
echo "<br>";
echo "sesudah di sorting ";
print_r(selection_sort($data));
?>

