<?php
echo "insertion sortn";
echo "<br>";
$data=array(3,4,2,5,1);
echo "sebelum di sorting ";
print_r($data);
function insertion_sort($data){
  $n=count($data);
  for ($i = 1;$i<$n;$i++){ for ($k = $i; $k>0; $k--) {
      if($data[$k]<$data[$k-1]){
        $dummy=$data[$k];
        $data[$k]=$data[$k-1];
        $data[$k-1]=$dummy;
      }
    }
  }
  return $data;
}
echo "<br>";
echo "sesudah di sorting ";
print_r(insertion_sort($data));
?>

