<?php
echo "bubble sort";
echo "<br>";
$data=array(3,4,2,5,1);
echo "sebelum di sorting ";
print_r($data);
function bubble_sort($data){
  $n=count($data);
  for ($i = 0;$i<$n;$i++){ for ($j = $n-1;$j>$i;$j--){
          if ($data[$j] < $data[$j-1]){ 
              $dummy=$data[$j];
              $data[$j]=$data[$j-1];
              $data[$j-1]=$dummy;
          }
      }    
  }
  return $data;
}
echo "<br>";
echo "setelah di sorting ";
print_r(bubble_sort($data));
?>
